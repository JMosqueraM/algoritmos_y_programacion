Algoritmo ejercicio_10
	descuento <- 0.15
	Escribir "Ingrese el valor de su compra"
	Leer valor
	total_sin_descuento <- valor
	total_con_descuento <- valor - (valor * 0.15)
	Escribir "Su total SIN descuento es de: " total_sin_descuento
	Escribir "Su total CON descuento es de: " total_con_descuento
	
FinAlgoritmo
