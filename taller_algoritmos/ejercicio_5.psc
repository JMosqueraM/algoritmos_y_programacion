Algoritmo ejercicio_5
	Escribir "Ingrese un numero a"
	Leer a
	Escribir "Ingrese otro numero b"
	Leer b
	suma <- a + b
	resta <- a - b
	division <- a / b
	multiplicacion <- a * b
	Escribir "La suma (" a " + " b ") = " suma
	Escribir "La resta (" a " - " b ") = " resta
	Escribir "La division (" a " / " b ") = " division
	Escribir "La multiplicacion (" a " * " b ") = " multiplicacion
	
FinAlgoritmo
